     <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script>
        let successMessage = "{{success}}"
        let errorMessage = "{{error}}"
        if(successMessage){
            Swal.fire({
            icon: "success",
            title: "Success",
            text: successMessage,
        });
        }
        if(errorMessage){
            Swal.fire({
            icon: "error",
